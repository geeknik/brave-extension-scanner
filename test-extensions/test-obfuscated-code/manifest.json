{
  "name": "TEST: Obfuscated Code Extension",
  "version": "1.0.0",
  "description": "Test extension with obfuscated code patterns for scanner validation",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["obfuscated.js"]
    }
  ]
}